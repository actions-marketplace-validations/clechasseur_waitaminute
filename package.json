{
  "name": "waitaminute",
  "version": "1.0.0-beta",
  "description": "GitHub Action to dismiss PR reviews when the PR diff changes",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "prepare-index": "ncc build index.js -o dist --license licenses.txt",
    "prepare-post": "ncc build post.js -o dist_post --license licenses.txt && node ./tools/copy-post-to-dist.js",
    "prepare": "npm run prepare-index && npm run prepare-post",
    "test": "jest",
    "all": "npm run lint && npm run prepare && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/clechasseur/waitaminute.git"
  },
  "keywords": [
    "GitHub",
    "Actions",
    "JavaScript"
  ],
  "author": "clechasseur",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/clechasseur/waitaminute/issues"
  },
  "homepage": "https://github.com/clechasseur/waitaminute#readme",
  "dependencies": {
    "@actions/artifact": "^1.1.1",
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "adm-zip": "^0.5.10"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.21.3",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.39.0",
    "jest": "^29.5.0"
  }
}
